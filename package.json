{
  "name": "memoryhole-legal-support-db",
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.2",
    "@redwoodjs/cli-data-migrate": "8.9.0",
    "@redwoodjs/cli-storybook-vite": "8.9.0",
    "@redwoodjs/core": "8.9.0",
    "@redwoodjs/project-config": "8.9.0",
    "@redwoodjs/realtime": "8.9.0",
    "@redwoodjs/studio": "13.0.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "conventional-changelog-cli": "^5.0.0",
    "graphql-codegen-typescript-mock-data": "^4.4.0",
    "node-ssh": "^13.2.1",
    "release-it": "^18.1.2",
    "storybook": "7.6.20"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true,
    "ignorePatterns": [
      "node_modules",
      "dist",
      "generated.ts"
    ],
    "rules": {
      "camelcase": "off",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ]
    }
  },
  "engines": {
    "node": "=20.x"
  },
  "prisma": {
    "seed": "yarn rw exec seed",
    "schema": "api/db/schema.prisma"
  },
  "version": "0.24.2",
  "packageManager": "yarn@4.9.4",
  "dependencies": {
    "axios": "^1.12.2",
    "axios-cookiejar-support": "^5.0.5",
    "pm2": "^6.0.13",
    "tough-cookie": "^5.1.2"
  },
  "scripts": {
    "release": "git stash && release-it --no-npm.publish && git stash pop",
    "refresh-mocks": "yarn graphql-codegen"
  }
}
